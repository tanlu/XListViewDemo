package com.miduo.financialmanageclient.bean;

import java.util.List;

public class MRepertory {
	private double assetTotalAmount;
	private double yesterdayRevenue;
	private double totalRevenue;
	private int assetTotalCount;
	private List<RepertoryItem> pcategoryList;

	public double getAssetTotalAmount() {
		return assetTotalAmount;
	}

	public void setAssetTotalAmount(double assetTotalAmount) {
		this.assetTotalAmount = assetTotalAmount;
	}

	public double getYesterdayRevenue() {
		return yesterdayRevenue;
	}

	public void setYesterdayRevenue(double yesterdayRevenue) {
		this.yesterdayRevenue = yesterdayRevenue;
	}

	public double getTotalRevenue() {
		return totalRevenue;
	}

	public void setTotalRevenue(double totalRevenue) {
		this.totalRevenue = totalRevenue;
	}

	public int getAssetTotalCount() {
		return assetTotalCount;
	}

	public void setAssetTotalCount(int assetTotalCount) {
		this.assetTotalCount = assetTotalCount;
	}

	public List<RepertoryItem> getPcategoryList() {
		return pcategoryList;
	}

	public void setPcategoryList(List<RepertoryItem> pcategoryList) {
		this.pcategoryList = pcategoryList;
	}

	@Override
	public String toString() {
		return "MRepertory [assetTotalAmount=" + assetTotalAmount
				+ ", yesterdayRevenue=" + yesterdayRevenue + ", totalRevenue="
				+ totalRevenue + ", assetTotalCount=" + assetTotalCount
				+ ", pcategoryList=" + pcategoryList + "]";
	}

}
