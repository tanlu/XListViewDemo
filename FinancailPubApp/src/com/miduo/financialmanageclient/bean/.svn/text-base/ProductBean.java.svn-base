package com.miduo.financialmanageclient.bean;

public class ProductBean {

	private String productCode;// 产品code
	private String productName;// 产品名称
	private double progress;// 销售进度
	private String expertRate;// 预期收益率
	private Double buyLimit;// 起售金额(元)
	private String productPeriod;// 产品期限,-99表示无期限
	private String feesRate;// 独立理财师费率（以%形式显示）
	private Integer productStatus;// 产品状态 10-预售 20-在售 30-售罄
	private Integer isHot;// 是否热销 0-否 1-是
	private String productHighlight;// 产品特点
	private String redeemDate;// 赎回日期
	private String unitPrice;// 单价
	private Integer productClassify;// 产品分类
	private Integer productType;//产品类型
	private String surplusAmount;//剩余额度
	
	
	
	public Integer getProductType() {
		return productType;
	}

	public void setProductType(Integer productType) {
		this.productType = productType;
	}

	public String getSurplusAmount() {
		return surplusAmount;
	}

	public void setSurplusAmount(String surplusAmount) {
		this.surplusAmount = surplusAmount;
	}

	public String getRedeemDate() {
		return redeemDate;
	}

	public void setRedeemDate(String redeemDate) {
		this.redeemDate = redeemDate;
	}

	public String getUnitPrice() {
		return unitPrice;
	}

	public void setUnitPrice(String unitPrice) {
		this.unitPrice = unitPrice;
	}

	public Integer getProductClassify() {
		return productClassify;
	}

	public void setProductClassify(Integer productClassify) {
		this.productClassify = productClassify;
	}

	public String getProductCode() {
		return productCode;
	}

	public void setProductCode(String productCode) {
		this.productCode = productCode;
	}

	public String getProductName() {
		return productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public double getProgress() {
		return progress;
	}

	public void setProgress(double progress) {
		this.progress = progress;
	}

	public String getExpertRate() {
		return expertRate;
	}

	public void setExpertRate(String expertRate) {
		this.expertRate = expertRate;
	}

	public Double getBuyLimit() {
		return buyLimit;
	}

	public void setBuyLimit(Double buyLimit) {
		this.buyLimit = buyLimit;
	}

	public String getProductPeriod() {
		return productPeriod;
	}

	public void setProductPeriod(String productPeriod) {
		this.productPeriod = productPeriod;
	}

	public String getFeesRate() {
		return feesRate;
	}

	public void setFeesRate(String feesRate) {
		this.feesRate = feesRate;
	}

	public Integer getProductStatus() {
		return productStatus;
	}

	public void setProductStatus(Integer productStatus) {
		this.productStatus = productStatus;
	}

	public Integer getIsHot() {
		return isHot;
	}

	public void setIsHot(Integer isHot) {
		this.isHot = isHot;
	}

	public String getProductHighlight() {
		return productHighlight;
	}

	public void setProductHighlight(String productHighlight) {
		this.productHighlight = productHighlight;
	}

	@Override
	public String toString() {
		return "ProductBaseBean [productCode=" + productCode + ", productName="
				+ productName + ", progress=" + progress + ", expertRate="
				+ expertRate + ", buyLimit=" + buyLimit + ", productPeriod="
				+ productPeriod + ", feesRate=" + feesRate + ", productStatus="
				+ productStatus + ", isHot=" + isHot + ", productHighlight="
				+ productHighlight + "]";
	}

}
