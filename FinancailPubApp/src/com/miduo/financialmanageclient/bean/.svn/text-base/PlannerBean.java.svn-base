package com.miduo.financialmanageclient.bean;

import java.io.Serializable;

/**
 * 选择理财顾问使用
 * 
 * @author huozhenpeng
 * 
 */
public class PlannerBean implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String name;
	private String certifiationlists;
	private String code;
	private boolean isopen;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCertifiationlists() {
		return certifiationlists;
	}

	public void setCertifiationlists(String certifiationlists) {
		this.certifiationlists = certifiationlists;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}
	

	public boolean isIsopen() {
		return isopen;
	}

	public void setIsopen(boolean isopen) {
		this.isopen = isopen;
	}

	@Override
	public String toString() {
		return "PlannerBean [name=" + name + ", certifiationlists="
				+ certifiationlists + ", code=" + code + "]";
	}

	
	

}
