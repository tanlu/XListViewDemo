<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <include layout="@layout/top_title_layout" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_29"
            android:layout_marginTop="@dimen/px2dp_54"
            android:text="@string/default_value"
            android:textColor="#333333"
            android:textSize="@dimen/px2dp_32" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/px2dp_29"
            android:layout_marginRight="@dimen/px2dp_30"
            android:background="#f5f5f5"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/px2dp_30"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    style="@style/TextStyle"
                    android:text="预期年化收益率" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    style="@style/TextStyle"
                    android:text="项目价值" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    style="@style/TextStyle"
                    android:text="预期剩余利息" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    style="@style/TextStyle"
                    android:text="剩余期限" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    style="@style/TextStyle"
                    android:text="最近赎回日" />
            </LinearLayout>

            <TextView
                android:layout_width="@dimen/px2dp_1"
                android:layout_height="@dimen/px2dp_320"
                android:background="#FFFFFF" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_01"
                    style="@style/TextStyle"
                    android:text="@string/default_value" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/tv_02"
                        style="@style/TextStyle"
                        android:text="@string/default_value"
                        android:textColor="#2ea7e0" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="元"
                        android:textColor="#2ea7e0" />
                </LinearLayout>

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    android:id="@+id/tv_03"
                    style="@style/TextStyle"
                    android:text="@string/default_value" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    android:id="@+id/tv_04"
                    style="@style/TextStyle"
                    android:text="@string/default_value" />

                <TextView
                    android:layout_width="@dimen/px2dp_400"
                    android:layout_height="@dimen/px2dp_1"
                    android:background="#FFFFFF" />

                <TextView
                    android:id="@+id/tv_05"
                    style="@style/TextStyle"
                    android:text="@string/default_value" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/px2dp_157"
        android:layout_marginLeft="@dimen/px2dp_30"
        android:layout_marginRight="@dimen/px2dp_30"
        android:text="您需要先指定收款银行卡信息,根据有关部门要求同卡进出的规定,此卡须与您的支付卡的信息一致."
        android:textColor="#2EA7E0"
        android:textSize="@dimen/px2dp_26" />

    <Button
        android:id="@+id/button"
        android:layout_width="@dimen/px2dp_660"
        android:layout_height="@dimen/px2dp_90"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/px2dp_20"
        android:background="@drawable/blue_round_bg"
        android:text="确认原支付卡"
        android:textColor="#FFFFFF" />

</RelativeLayout>