<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="#ffffff">

    <include
        android:id="@+id/top_layout"
        layout="@layout/top_title_layout" />

    <ImageView
        android:id="@+id/commite_img"
        android:layout_width="@dimen/px2dp_60"
        android:layout_height="@dimen/px2dp_60"
        android:layout_below="@id/top_layout"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/px2dp_110"
        android:src="@drawable/blue_commite" />

    <TextView
        android:id="@+id/commite_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/commite_img"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/px2dp_16"
        android:text="您的赎回申请已成功提交！"
        android:textColor="#2ea7e0"
        android:textSize="@dimen/px2sp_40" />

    <View
        android:id="@+id/wait_line"
        android:layout_width="1dp"
        android:layout_height="@dimen/px2dp_85"
        android:layout_below="@id/commite_txt"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/px2dp_16"
        android:layout_marginTop="@dimen/px2dp_24" />

    <ImageView
        android:id="@+id/wait_img"
        android:layout_width="@dimen/px2dp_60"
        android:layout_height="@dimen/px2dp_60"
        android:layout_below="@id/wait_line"
        android:layout_centerHorizontal="true"
        android:src="@drawable/grey_wait" />

    <TextView
        android:id="@+id/wait_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/wait_img"
        android:layout_marginLeft="@dimen/px2dp_130"
        android:layout_marginRight="@dimen/px2dp_130"
        android:layout_marginTop="@dimen/px2dp_16"
        android:lineSpacingExtra="@dimen/px2dp_16"
        android:text="米多平台正在焦急等待银行对账信息，成功赎回后您的资金将在--个工作日内到达您的指定的收款账号"
        android:textColor="#999999"
        android:textSize="@dimen/px2sp_24" />

    <View
        android:id="@+id/success_line"
        android:layout_width="1dp"
        android:layout_height="@dimen/px2dp_85"
        android:layout_below="@id/wait_txt"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/px2dp_16"
        android:layout_marginTop="@dimen/px2dp_20" />

    
    <ImageView
        android:id="@+id/success_img"
        android:layout_width="@dimen/px2dp_60"
        android:layout_height="@dimen/px2dp_60"
        android:layout_below="@id/success_line"
        android:layout_centerHorizontal="true"
        android:src="@drawable/grey_success" />

    <TextView
        android:id="@+id/success_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/success_img"
        android:layout_marginLeft="@dimen/px2dp_110"
        android:layout_marginRight="@dimen/px2dp_110"
        android:layout_marginTop="@dimen/px2dp_16"
        android:lineSpacingExtra="@dimen/px2dp_16"
        android:text="赎回成功后，您可以在“米仓”-“米多总资产”-“历史订单”中查看已赎回的产品"
        android:textColor="#999999"
        android:textSize="@dimen/px2sp_24" />

    <TextView
        android:id="@+id/btn_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/px2dp_50"
        android:layout_marginLeft="@dimen/px2dp_30"
        android:layout_marginRight="@dimen/px2dp_30"
        android:background="@drawable/blue_round_bg"
        android:gravity="center"
        android:paddingBottom="@dimen/px2dp_26"
        android:paddingTop="@dimen/px2dp_26"
        android:text="我知道了"
        android:textColor="#ffffff" />
    

</RelativeLayout>