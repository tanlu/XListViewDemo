<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:customer="http://schemas.android.com/apk/res/com.miduo.financialmanageclient"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <include
        android:id="@+id/top_layout"
        android:layout_height="match_parent"
        layout="@layout/top_title_layout" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/px2dp_58" >

        <TextView
            android:id="@+id/info_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:text="基于您的回答为您提供全方位的保险规划"
            android:textColor="#666666"
            android:textSize="@dimen/px2sp_32" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_alignLeft="@id/info_txt"
            android:layout_alignRight="@id/info_txt"
            android:layout_below="@id/info_txt"
            android:layout_marginTop="@dimen/px2dp_20"
            android:background="#ececec"
            android:paddingLeft="@dimen/px2dp_10"
            android:paddingRight="@dimen/px2dp_10" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="@dimen/px2dp_82"
        android:layout_marginRight="@dimen/px2dp_82"
        android:layout_marginTop="@dimen/px2dp_60"
        android:background="#1cc8e9" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px2dp_62"
        android:layout_marginLeft="@dimen/px2dp_82"
        android:layout_marginRight="@dimen/px2dp_82"
        android:orientation="horizontal" >

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#1cc8e9" />

        <TextView
            android:id="@+id/owner_txt"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/bg_blue_right_angle"
            android:gravity="center"
            android:text="本\u3000人"
            android:textColor="#ffffff"
            android:textSize="@dimen/px2sp_32" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#1cc8e9" />

        <TextView
            android:id="@+id/spouse_txt"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="配\u3000偶"
            android:textColor="#666666"
            android:textSize="@dimen/px2sp_32" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#1cc8e9" />

        <TextView
            android:id="@+id/child_txt"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="子\u3000女"
            android:textColor="#666666"
            android:textSize="@dimen/px2sp_32" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#1cc8e9" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="@dimen/px2dp_82"
        android:layout_marginRight="@dimen/px2dp_82"
        android:background="#1cc8e9" />

    <com.miduo.financialmanageclient.widget.InvestmentBarPic
        android:id="@+id/investmentBarPic"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px2dp_434"
        android:layout_marginTop="@dimen/px2dp_118"
        android:background="@drawable/testresult" 
        customer:leftPadding="@dimen/px2dp_80"
        customer:perBarPadding="@dimen/px2dp_54"
        
        >
    </com.miduo.financialmanageclient.widget.InvestmentBarPic>
     <com.miduo.financialmanageclient.widget.InvestmentBarPic
        android:visibility="gone"
        android:id="@+id/investmentBarPic2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px2dp_434"
        android:layout_marginTop="@dimen/px2dp_118"
        android:background="@drawable/testresult" 
        customer:leftPadding="@dimen/px2dp_156"
        customer:perBarPadding="@dimen/px2dp_54"
        
        >
    </com.miduo.financialmanageclient.widget.InvestmentBarPic>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px2dp_1"
        android:background="#0d9fd2" />

    <LinearLayout
        android:id="@+id/ll_layout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px2dp_52"
        android:layout_marginRight="@dimen/px2dp_52"
        android:layout_marginTop="@dimen/px2dp_14"
        android:orientation="horizontal" >

        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="定期寿险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />

        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="意外险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />

        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="重疾险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />

        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="医疗险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />
    </LinearLayout>
    
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/ll_layout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px2dp_130"
        android:layout_marginRight="@dimen/px2dp_130"
        android:layout_marginTop="@dimen/px2dp_14"
        android:orientation="horizontal" >
        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="意外险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />

        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="重疾险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />

        <TextView
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="医疗险"
            android:textColor="#333333"
            android:textSize="@dimen/px2sp_24" />
    </LinearLayout>
    
    <View 
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/px2dp_30"
        android:layout_marginRight="@dimen/px2dp_30"
        android:layout_marginBottom="@dimen/px2dp_78"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/repeat_test_txt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/blue_round_bg"
            android:gravity="center"
            android:paddingBottom="@dimen/px2dp_20"
            android:paddingTop="@dimen/px2dp_20"
            android:text="重新测试"
            android:textColor="#ffffff"
            android:textSize="@dimen/px2sp_34" />

        <View
            android:layout_width="@dimen/px2dp_20"
            android:layout_height="1dp"
            android:background="#ffffff" />

        <TextView
            android:id="@+id/ask_txt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/blue_round_bg"
            android:gravity="center"
            android:paddingBottom="@dimen/px2dp_20"
            android:paddingTop="@dimen/px2dp_20"
            android:text="看看产品"
            android:textColor="#ffffff"
            android:textSize="@dimen/px2sp_34" />
    </LinearLayout>

</LinearLayout>