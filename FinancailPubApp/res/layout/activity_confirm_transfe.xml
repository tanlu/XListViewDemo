<com.miduo.financialmanageclient.widget.MyRelaLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <include
        android:id="@+id/top_layout"
        layout="@layout/top_title_layout" />

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px2dp_86"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="@dimen/px2dp_30"
        android:layout_marginLeft="@dimen/px2dp_29"
        android:layout_marginRight="@dimen/px2dp_29"
        android:background="@drawable/button_bg_blue"
        android:text="同意协议并确认转账"
        android:textColor="#FEFFFF" />

    <com.miduo.financialmanageclient.widget.MyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/button"
        android:layout_below="@id/top_layout" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/px2dp_30"
                android:layout_marginLeft="@dimen/px2dp_29"
                android:layout_marginTop="@dimen/px2dp_30"
                android:text="@string/default_value"
                android:textColor="#333333"
                android:textSize="@dimen/px2dp_32" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/px2dp_29"
                android:layout_marginRight="@dimen/px2dp_30"
                android:background="#f5f5f5"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px2dp_30"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <TextView
                        style="@style/TextStyle"
                        android:text="预期年化收益率" />

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <TextView
                        style="@style/TextStyle"
                        android:text="项目价值" />

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <TextView
                        style="@style/TextStyle"
                        android:text="预期剩余利息" />

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <TextView
                        style="@style/TextStyle"
                        android:text="剩余期限" />

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <TextView
                        style="@style/TextStyle"
                        android:text="最近赎回日" />
                </LinearLayout>

                <TextView
                    android:layout_width="@dimen/px2dp_2"
                    android:layout_height="@dimen/px2dp_320"
                    android:background="#FFFFFF" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tv_01"
                        style="@style/TextStyle"
                        android:layout_marginLeft="@dimen/px2dp_30"
                        android:text="@string/default_value" />

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <LinearLayout
                        style="@style/TextStyle"
                        android:layout_marginLeft="@dimen/px2dp_30"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_02"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/default_value"
                            android:textColor="#2ea7e0"
                            android:textSize="@dimen/px2dp_28" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="元"
                            android:textColor="#2ea7e0"
                            android:textSize="@dimen/px2dp_28" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <LinearLayout
                        style="@style/TextStyle"
                        android:layout_marginLeft="@dimen/px2dp_30"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_03"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/default_value"
                            android:textSize="@dimen/px2dp_28" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="元"
                            android:textSize="@dimen/px2dp_28" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <TextView
                        android:id="@+id/tv_04"
                        style="@style/TextStyle"
                        android:layout_marginLeft="@dimen/px2dp_30"
                        android:text="@string/default_value" />

                    <TextView
                        android:layout_width="@dimen/px2dp_400"
                        android:layout_height="@dimen/px2dp_2"
                        android:background="#FFFFFF" />

                    <TextView
                        android:id="@+id/tv_05"
                        style="@style/TextStyle"
                        android:layout_marginLeft="@dimen/px2dp_30"
                        android:text="@string/default_value" />
                </LinearLayout>
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/px2dp_30"
                android:layout_marginTop="@dimen/px2dp_50"
                android:focusable="true"
                android:focusableInTouchMode="true" >

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/px2dp_18"
                    android:text="转让价格"
                    android:textSize="@dimen/px2dp_32" />

                <ImageView
                    android:id="@+id/delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="@dimen/px2dp_30"
                    android:layout_marginTop="@dimen/px2dp_22"
                    android:src="@drawable/delete" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView4"
                    android:layout_marginTop="@dimen/px2dp_61"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:paddingRight="@dimen/px2dp_7"
                            android:text="我的收益率"
                            android:textColor="#2EA7E0"
                            android:textSize="@dimen/px2dp_26" />

                        <TextView
                            android:id="@+id/my_money"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/image_bg_blue"
                            android:gravity="center"
                            android:text="@string/default_value"
                            android:textColor="#2EA7E0"
                            android:textSize="@dimen/px2dp_26" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/px2dp_15"
                            android:paddingRight="@dimen/px2dp_7"
                            android:text="对方收益率"
                            android:textColor="#80c209"
                            android:textSize="@dimen/px2dp_26" />

                        <TextView
                            android:id="@+id/you_money"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/image_bg_green"
                            android:gravity="center"
                            android:text="@string/default_value"
                            android:textColor="#80c209"
                            android:textSize="@dimen/px2dp_26" />
                    </LinearLayout>
                </LinearLayout>

                <EditText
                    android:id="@+id/big_price"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="@dimen/px2dp_14"
                    android:layout_marginRight="@dimen/px2dp_60"
                    android:layout_toRightOf="@+id/textView1"
                    android:background="@null"
                    android:inputType="numberDecimal"
                    android:textColor="#2DA7E1"
                    android:textSize="@dimen/px2dp_54" >
                </EditText>

                <ImageView
                    android:id="@+id/delete"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="@dimen/px2dp_30"
                    android:layout_marginTop="@dimen/px2dp_22"
                    android:src="@drawable/delete" />

                <LinearLayout
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/small_price"
                    android:layout_marginLeft="@dimen/px2dp_166"
                    android:layout_marginTop="@dimen/px2dp_36"
                    android:drawableLeft="@drawable/remind"
                    android:drawablePadding="@dimen/px2dp_10"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/remind" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/px2dp_6"
                        android:text="建议在"
                        android:textColor="#666666"
                        android:textSize="@dimen/px2dp_20" />

                    <TextView
                        android:id="@+id/min_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/default_amount"
                        android:textColor="#666666"
                        android:textSize="@dimen/px2dp_20" />

                    <TextView
                        android:id="@+id/min_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="元-"
                        android:textColor="#666666"
                        android:textSize="@dimen/px2dp_20" />

                    <TextView
                        android:id="@+id/max_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/default_amount"
                        android:textColor="#666666"
                        android:textSize="@dimen/px2dp_20" />

                    <TextView
                        android:id="@+id/min_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="元之间"
                        android:textColor="#666666"
                        android:textSize="@dimen/px2dp_20" />
                </LinearLayout>

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="@dimen/px2dp_32"
                    android:layout_height="@dimen/px2dp_32"
                    android:layout_alignBaseline="@+id/big_price"
                    android:layout_alignBottom="@+id/big_price"
                    android:layout_marginLeft="@dimen/px2dp_10"
                    android:layout_toRightOf="@+id/textView2"
                    android:text="￥"
                    android:textColor="#2ea7e0"
                    android:textSize="@dimen/px2dp_32" />

                <TextView
                    android:id="@+id/small_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/big_price"
                    android:layout_below="@+id/big_price"
                    android:layout_marginTop="@dimen/px2dp_4"
                    android:text="@string/default_value"
                    android:textColor="#2ea7e0"
                    android:textSize="@dimen/px2sp_26" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px2dp_190"
                android:layout_above="@id/button"
                android:layout_marginLeft="@dimen/px2dp_29"
                android:layout_marginRight="@dimen/px2dp_29"
                android:layout_marginTop="@dimen/px2dp_66"
                android:background="#F5F5F5"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/px2dp_30"
                    android:text="24小时候后无人接单，自动取消转让"
                    android:textSize="@dimen/px2dp_24" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/px2dp_20"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="手续费=转让价格×0.25%="
                        android:textColor="#42ACE3"
                        android:textSize="@dimen/px2dp_24" />

                    <TextView
                        android:id="@+id/poundage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/default_amount"
                        android:textColor="#42ACE3"
                        android:textSize="@dimen/px2dp_24" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="元"
                        android:textColor="#42ACE3"
                        android:textSize="@dimen/px2dp_24" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginBottom="@dimen/px2dp_20"
                    android:layout_marginTop="@dimen/px2dp_10"
                    android:text="转让成功后收取，未转让成功则不收取"
                    android:textColor="#42ACE3"
                    android:textSize="@dimen/px2dp_24" />
            </LinearLayout>
        </LinearLayout>
    </com.miduo.financialmanageclient.widget.MyScrollView>

</com.miduo.financialmanageclient.widget.MyRelaLayout>