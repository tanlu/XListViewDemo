<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GesturePasswordView">
        <attr name="bigView" format="boolean"></attr>
    </declare-styleable>

    <attr name="checked" format="boolean" />
    <attr name="img_checked" format="reference" />
    <attr name="img_unchecked" format="reference" />
    <attr name="textcolor_checked" format="color" />
    <attr name="textcolor_unchecked" format="color" />
    <attr name="text_size" format="dimension" />
    <attr name="text_content" format="string" />

    <declare-styleable name="custom_radio">
        <attr name="checked" />
        <attr name="img_checked" />
        <attr name="img_unchecked" />
        <attr name="textcolor_checked" />
        <attr name="textcolor_unchecked" />
        <attr name="text_size" />
        <attr name="text_content" />
    </declare-styleable>
    <declare-styleable name="ring_view">

        <!-- 字体颜色 -->
        <attr name="ring_text_color" format="color" />
        <!-- 小字号字体大小 -->
        <attr name="small_text_size" format="dimension" />
        <!-- 大字号字体大小 -->
        <attr name="big_text_size" format="dimension" />
        <!-- 大圆环宽度 -->
        <attr name="big_ring_width" format="dimension" />
        <!-- 小圆环宽度 -->
        <attr name="small_ring_width" format="dimension" />
        <!-- 间隔环宽度 -->
        <attr name="interval_ring_width" format="dimension" />
        <!-- 小圆环宽度 -->
        <attr name="is_show" format="boolean" />
    </declare-styleable>
    <declare-styleable name="waveTextView">
        <attr name="textSize" format="dimension" />
        <attr name="paintWidth" format="dimension" />
        <attr name="symbolSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <attr name="inwidth" format="dimension" />
    <attr name="inheight" format="dimension" />
    <attr name="specing" format="dimension" />
    <attr name="textsize" format="dimension" />
    <attr name="div_" format="dimension" />
    <attr name="innercell" format="dimension" />
    <attr name="topcolor" format="color" />
    <attr name="bgcolor" format="color" />
    <attr name="stroke" format="dimension" />
    <attr name="radius" format="dimension" />

    <declare-styleable name="percentage">
        <attr name="topcolor" />
        <attr name="bgcolor" />
        <attr name="stroke" />
        <attr name="radius" />
    </declare-styleable>
    <declare-styleable name="protopview">
        <attr name="inwidth" />
        <attr name="inheight" />
        <attr name="specing" />
        <attr name="textsize" />
        <attr name="div_" />
        <attr name="innercell" />
    </declare-styleable>
    <declare-styleable name="investmentBarPic">
        <attr name="leftPadding" format="dimension"></attr>
        <attr name="perBarPadding" format="dimension"></attr>
    </declare-styleable>
    <attr name="text" format="string" />
    <attr name="radius1" format="dimension"/>
    <attr name="textsize1" format="dimension"/>

    <declare-styleable name="message">
        <attr name="text" />
        <attr name="radius1" />
        <attr name="textsize1" />
    </declare-styleable>

</resources>